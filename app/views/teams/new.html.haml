%h1 New team

- form_for(@team) do |f| 
  = f.error_messages
  %p
    = f.label 'title'
    = f.text_field 'title'
  %p 
    = f.label 'description'
    = f.text_field 'description'
  %p
    = f.label 'alias'
    = f.text_field 'alias'
  %p
    %input(type="hidden" name="team[leader_id]" value="#{@session}")
  %p 
    = f.submit 'Create'
= link_to 'Back', teams_path
